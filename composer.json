{
    "name": "qiq/helper-sapien",
    "type": "library",
    "description": "Provides Qiq helpers for Sapien Request and Response objects.",
    "keywords": [
        "sapien",
        "request",
        "response",
        "qiq",
        "helper"
    ],
    "homepage": "https://github.com/qiqphp/helper-sapien",
    "license": "MIT",
    "authors": [
        {
            "name": "Qiq Helper Sapien Contributors",
            "homepage": "https://github.com/qiqphp/helper-sapien/contributors"
        }
    ],
    "require": {
        "php": "^8.1",
        "qiq/qiq": "2.x@dev",
        "sapien/sapien": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Qiq\\Helper\\Sapien\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^1.0",
        "pds/skeleton": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Qiq\\Helper\\Sapien\\": "tests/"
        }
    },
    "scripts": {
        "check": "composer cs-fix && composer test",
        "cs-fix": "./vendor/bin/phpstan analyze -c phpstan.neon --memory-limit=1G",
        "test": "./vendor/bin/phpunit"
    }
}
